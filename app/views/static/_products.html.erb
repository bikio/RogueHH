  <div id= "products" name="products">
    <ul class="products" id="products">
      <% @products.each do |product| %>
      <li>
        <%= image_tag(product.image.url, class: 'prod-img') %>
          <h2><%= product.name %></h2>
          <p><%= product.description %></p>
          <p>$<%= product.price %> </p>
          <!-- <p> <%= product.colors %> </p> -->
          <p> <a class="btn btn-primary"<!-- <%=product.id%>-->I Want It Nah</a> </p>
      <% end %>
      </li>
    </ul>
  </div>
